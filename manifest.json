{
	"manifest_version" : 2,
	"name" : "pitchShift",
	"version" : "1.0",
	"description" : "A simple pitch audio pitch shifter",
	"permissions" : ["activeTab", "tabs"],
	"content_scripts" : [
		{
			"matches" : ["<all_urls>"],
			"js" : ["content.js"]
		}
	},
	"browser_action" : {
		"default_title" : "Pitch Shifter"
	}
	
}
